   "source": [
    "import urllib2\n",
    "import bs4\n",
    "\n",
    "url = \"https://www.econet.co.zw/\"\n",
    "x = urllib2.urlopen(url)\n",
    "data = x.read()\n",
    "x.close()\n",
    "soupdata = bs4.BeautifulSoup(data)\n",
    "y = [link.get('href') for link in soupdata.find_all('a')]\n",
    "y[0:15]"
   ]
